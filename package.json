    "react-dom": "^15.0.0",
    "snapsvg": "^0.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.3.13",
    "chai": "^3.2.0",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "gulp": "^3.8.11",
    "gulp-mocha": "^2.2.0",
