  },
  "dependencies": {
    "browserify-optional": "^1.0.0",
    "classnames": "^2.1.1",
    "snapsvg": "^0.5.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.2.0",
    "cross-env": "^5.0.0",
    "eslint": "^3.0.0",
    "eslint-plugin-react": "^7.0.1",
    "gulp": "^3.8.11",
    "gulp-git": "^2.2.0",
    "gulp-mocha": "^4.3.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.4.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-component-gulp-tasks": "^0.7.1",
    "react-dom": "^15.5.4",
    "sinon": "^2.2.0"
  },
  "peerDependencies": {
    "react": ">=0.14.0 <16.0.0",
    "react-dom": ">=0.14.0 <16.0.0"
  },
  "browserify": {
    "transform": [
      "browserify-optional"
    ]
  },
  "browserify-shim": {
    "react": "global:React",
    "react-dom": "global:ReactDOM",
    "snapsvg": "global:Snap"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production gulp clean && gulp build",
    "examples": "gulp dev:server",
    "lint": "eslint ./; true",
    "publish:site": "gulp publish:examples",
    "publish:version": "gulp commit:version && gulp push",
    "start": "gulp dev",
    "test": "gulp test --reporter dot",
    "tdd": "gulp watch:tests --reporter spec",
    "watch": "gulp watch:lib",
    "release": "npm run build && gulp bump && npm run publish:version && gulp release",
    "release:minor": "npm run build && gulp bump:minor && npm run publish:version && gulp release",
    "release:major": "npm run build && gulp bump:major && npm run publish:version && gulp release"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "readmeFilename": "README.md",
  "keywords": [
    "react",
    "react-component"
  ],
